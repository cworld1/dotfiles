alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

# System
alias where whereis
alias rmf 'rm -rf'
alias mkdir 'mkdir -v -p'
alias wget 'wget --hsts-file="$XDG_DATA_HOME/wget-hsts"'

# Shell
alias unset="set --erase"
alias :q exit
alias :Q exit
alias cls clear

# User
alias clean~ xdg-ninja
alias osfetch fastfetch
alias gitfetch onefetch
